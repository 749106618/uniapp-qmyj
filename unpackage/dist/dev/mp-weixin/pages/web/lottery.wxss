
.wwi-slider-rolling{overflow: hidden; position: relative; width: 100%;}
.wwi-slider-box{position: absolute;}


page{background: #f8f8f8;}
.m-ui-dial {width: 700rpx;height: 700rpx;margin: 50rpx auto;position: relative;}
.pointer {position: absolute;top: 50%;left: 50%;display: block;width: 210rpx;height: 315rpx;margin: -157rpx 0 0 -102rpx;-webkit-transform-origin: 102rpx 193rpx;-ms-transform-origin: 102rpx 193rpx;transform-origin: 102rpx 193rpx;}
.pointer .btn {position: absolute;left: 0;display: block;padding: 0;background-color: initial;border: none;top: 80rpx;width: 210rpx;height: 210rpx;border-radius: 50%;}
.winner-name{ height:11rem; padding:5px 15px 15px; background-color: #fff; font-size: 12px; border-radius: 6px; border: 1px solid #f0f0f0; margin:20px; overflow:hidden;}
.winner-list{position: relative; height: 140px; margin: 0 5px; overflow: hidden;}
.winner-list::before{content: ""; position: absolute; top: 0; width: 100%; height: 40px; position: absolute; z-index: 1; background: url(data:image/svg+xml;base64,);background: -webkit-gradient(linear,  left top, left bottom,  from(rgba(255,255,255,1)),to(rgba(255,255,255,0)));background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 );}
.list-wrap{display: none; position: absolute; top: 0px; width: 100%;}
.winner-title{position: relative; height: 40px; line-height: 40px; text-align: center;}
.winner-title-bg{position: absolute; top: 19px; left: 0; width: 100%; border-top: 2px solid #f34c49;}
.winner-title-con{position: relative; display: inline-block;  padding: 0 10px; font-size:1rem; background-color: #fff; color: #f34c49;}
.winner-inner-tit{overflow: hidden; line-height: 36px;}
.winner-name .span{float: left; width: 39%; padding: 0 5%; white-space: nowrap; overflow: hidden; text-align:center;-o-text-overflow:ellipsis;text-overflow:ellipsis;
}
.winner-info{line-height: 38px; border-top: 1px dashed #d9d8d8; overflow: hidden;}
.border-none{border: 0;}
.w-zm{font-size: 16px; color: #9d4e21;}
.w-name{color: #928977; -webkit-box-flex: 1; -webkit-flex: 1; -ms-flex: 1; flex: 1;}
.w-prize{color: #f34c49; -webkit-box-flex: 1; -webkit-flex: 1; -ms-flex: 1; flex: 1;}
.winner-none{text-align: center; line-height: 300px; color: #666; font-size: 14px;}

