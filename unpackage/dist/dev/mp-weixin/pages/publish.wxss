
page{background-color: #F8F8F8;height: auto; min-height: 100%;width: 750rpx; margin: 0 auto;}
.wwi-icon-list{  padding:40rpx 20rpx 20rpx; -webkit-flex-wrap: wrap; -ms-flex-wrap: wrap; flex-wrap: wrap; position: relative; z-index: 1;}
.wwi-icon-list .li{ width: 142rpx; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; -webkit-box-pack: center; -webkit-justify-content: center; -ms-flex-pack: center; justify-content: center; -webkit-box-orient: vertical; -webkit-box-direction: normal; -webkit-flex-direction: column; -ms-flex-direction: column; flex-direction: column; -webkit-box-align: center; -webkit-align-items: center; -ms-flex-align: center; align-items: center; margin-bottom: 20rpx;
}
.wwi-icon-list .li .wwi-icon{width: 60rpx; height: 60rpx; padding: 20rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex; -webkit-box-pack: center; -webkit-justify-content: center; -ms-flex-pack: center; justify-content: center; -webkit-box-align: center; -webkit-align-items: center; -ms-flex-align: center; align-items: center;background-position: 50%; border-radius:60rpx; position: relative; z-index: 1;}
.wwi-icon-list .li .span{font-size: 26rpx; margin-top: 10rpx;}
.wwi-icon-list .li .s1{background: #d6edeb;}
.wwi-icon-list .li .s2{background: #abc6b5;}
.wwi-icon-list .li .s3{background: #e7d4e5;}
.wwi-icon-list .li .s4{background: #ead7d3;}
.wwi-icon-list .li .s5{background: #e5e5e5;}
.wwi-publish-fbxq{background: #FFFFFF; margin: 20rpx; padding: 30rpx;border-radius:15rpx;-webkit-box-shadow: 0px 0px 20rpx 0px #82d8b0;box-shadow: 0px 0px 20rpx 0px #82d8b0;}
.wwi-publish-fbxq .h3{font-size: 38rpx; color: #2cb476; font-weight: bold;}
.wwi-publish-fbxq .title{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex; -webkit-box-orient: horizontal; -webkit-box-direction: normal; -webkit-flex-direction: row; -ms-flex-direction: row; flex-direction: row; -webkit-box-pack: center; -webkit-justify-content: center; -ms-flex-pack: center; justify-content: center; -webkit-box-align: center; -webkit-align-items: center; -ms-flex-align: center; align-items: center; font-size: 32rpx; height: 80rpx; color: #333; font-weight: bold;}
.wwi-publish-fbxq .title:before{content: ''; width: 50rpx; height: 2rpx; background: #333; margin-right: 0.25em;}
.wwi-publish-fbxq .title:after{content: ''; width: 50rpx; height: 2rpx; background: #333; margin-left: 0.25em;}
.wwi-publish-fbxq .span{line-height: 60rpx;}
.wwi-publish-fbxq .span .num{color: #2CB476; font-weight: bold ;}
.wwi-publish-fbxq .section input{background: #F8F8F8; padding:10rpx 20rpx;}
.wwi-publish-fbxq .submit{background: #2cb476; color: #FFFFFF; font-weight:bold ;}
.wwi-publish-re{margin: 20rpx;}
.wwi-publish-re .title{font-size: 32rpx; line-height: 100rpx; font-weight: bold;}
.wwi-publish-re .list{}
.wwi-publish-re .list .li{width: 345rpx; margin-bottom: 20rpx;}
.wwi-publish-re .list .li:nth-child(even){margin-left: 20rpx;}
.wwi-publish-re .list .li .img{width: 345rpx; height: 240rpx; border-radius: 15rpx;}
.wwi-publish-re .list .li .img .picSwipe{border-radius: 15rpx; overflow: hidden;height: 240rpx;}
.wwi-publish-re .list .li .img .imgs image{ vertical-align: middle; height: 240rpx;}
.wwi-publish-re .list .li .store-name{font-size: 28rpx; font-weight: bold; margin: 15rpx 0;}
.wwi-publish-re .list .li .store-name image{width: 50rpx; height: 50rpx; border-radius: 50rpx; margin-right: 15rpx;}

